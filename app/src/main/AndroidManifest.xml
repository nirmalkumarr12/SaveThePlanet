<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="savetheplanet.team8.project.com.savetheplanet">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SignInGoogleEmailActivity" />
        <activity android:name=".activity.SignUpEmailActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SignInSignUpActivity" />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.SignInEmailActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.ProductRegistration">

        </activity>
        <activity android:name=".activity.activity_list_products">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>